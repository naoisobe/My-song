<div class="container">
  <div class="row">
    <div class="col-4">
      <%= render "users/sidebar", user: @user %>
    </div>
    <div class="col-8">
      <% @song.each do |song| %>
        <%= link_to song_path(song) do %>
          <%= image_tag song.thumbnail.to_s, size: '200x100' %>
          <%= song.title %>
          <%= song.user.name%>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
 