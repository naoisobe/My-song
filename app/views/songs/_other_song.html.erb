<% songs.each do |song| %>
  <%= link_to song_path(song) do %>
    <div class="song-card other-song-card">
      <%= image_tag song.thumbnail.to_s, class:"thumbnail" %>
      <div class="card-body", style="height: initial;">
        <h5><%= song.title %></h5>
      </div>
    </div>
  <% end %>
<% end %>
