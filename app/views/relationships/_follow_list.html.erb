<div class="channel">
  <div>
    <%= link_to song_my_list_path(user) do %>
      <%= image_tag user.profile_image.to_s, class:"user-profile" %>
      <%= user.name %>
    <% end %>
  </div>
    <div id="follow_list_<%= user.id %>">
      <div class="follow-btn"><%= render 'follow_button', user: user, relation: @relation, follow: @new_follow %></div>
    </div>
</div>
