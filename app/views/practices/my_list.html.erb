<div class="container">
  <div class="row">
    <div class="col-4">
      <%= render "users/sidebar", user: @user %>
    </div>
    <div class="col-8">
      <% @practices.each do |practice| %>
        <%= link_to practice_path(practice) do %>
        <%= video_tag practice.video_url.to_s, controls: true, width: 550 %>
          <%= practice.title %>
          <%= practice.user.name%>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
 