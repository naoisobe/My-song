<%= search_form_for @q, url:songs_search_path, class:"search_form" do |f| %>
  <%= f.search_field :title_cont, placeholder:"検索",class:"search-bar"%>
  <%= f.button :type => "submit", class:"btn search-btn", style: "border-style: none;" do %>
    <%= icon("fas","search", class:"fa-2x") %>
  <% end %>
<% end %>
